{
  "name": "@vialabs-io/npm-registry",
  "version": "2.0.0",
  "description": "Central repository for blockchain-related configurations (excluding Solidity contracts)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "postinstall": "ls -la && node ./scripts/generateChainConfig.js && npx tsc -p ./tsconfig.json"
  },
  "keywords": [
    "blockchain",
    "configuration",
    "ethereum",
    "chain"
  ],
  "author": "Atlas VIA Labs",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.31",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.9.2"
},
"dependencies": {
    "axios": "^1.7.7",
    "ethers": "^6.13.4",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist",
    "src",
    "scripts",
    "tsconfig.json"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.ts?(x)",
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
